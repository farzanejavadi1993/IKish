<?xml version="1.0" encoding="utf-8"?>
<layout>
    <RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:background="@color/pale_gray"
        xmlns:mstb="http://schemas.android.com/apk/res-auto"
        android:layout_height="match_parent"
        tools:context=".ui.activities.TestActivity">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/passenger_toolbar"
            android:layout_width="match_parent"
            android:layout_height="@dimen/toolbar_size">


            <TextView
                android:textStyle="bold"
                android:textColor="@color/medium_color"
                android:textSize="@dimen/passenger_title_size"
                android:lineSpacingExtra="19sp"
                android:id="@+id/tv_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/add_new_passenger"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:clickable="true"
                android:background="?attr/selectableItemBackgroundBorderless"
                android:focusable="true"
                android:id="@+id/iv_close"
                android:layout_width="@dimen/image_size"
                android:layout_height="@dimen/image_size"
                android:layout_marginEnd="@dimen/margin_normal"
                android:src="@drawable/ic_close"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>
        <ScrollView
            android:layout_below="@+id/passenger_toolbar"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <LinearLayout
                    android:layout_marginTop="18dp"
                    android:elevation="4dp"
                    android:orientation="horizontal"
                    android:background="@drawable/back_tabs"
                    android:id="@+id/toggleButton"
                    android:layout_width="0dp"

                    android:layout_height="38dp"
                    android:layout_marginStart="22dp"
                    android:layout_marginEnd="22dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" >
                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/back_tabs">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <TextView

                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:text=""
                               />

                            <TextView
                                android:id="@+id/select"
                                android:background="@drawable/back_select"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:text="" />

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">
                            <TextView
                                android:textColor="@color/invalid_name_color"
                                android:lineSpacingExtra="9sp"
                                android:textSize="13sp"
                                android:id="@+id/other_country"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:text="@string/other_country"

                                android:gravity="center"/>

                            <TextView
                                android:lineSpacingExtra="9sp"
                                android:textSize="13sp"
                                android:textColor="@color/topaz"
                                android:id="@+id/iranian"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:text="@string/iranian"
                                android:gravity="center"/>

                        </LinearLayout>
                    </FrameLayout>


                </LinearLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/layout_first_name"
                    android:layout_width="0dp"
                    android:layout_height="75dp"
                    android:layout_marginStart="@dimen/margin_normal"
                    android:layout_marginTop="21dp"
                    android:layout_marginEnd="@dimen/margin_normal"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/toggleButton" >
                    <TextView
                        style="@style/H7Right"
                        android:id="@+id/tv_first_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/new_passenger_first_name"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />
                    <com.google.android.material.card.MaterialCardView
                        android:id="@+id/card_first_name"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/tv_first_name"
                        app:strokeColor="#dfdfdf"
                        app:strokeWidth="1dp"
                        app:cardCornerRadius="10dp"
                        app:cardBackgroundColor="#e6e5ee"
                        android:layout_width="0dp"
                        android:layout_height="48dp"
                        android:layout_marginTop="5dp"
                        >
                        <com.google.android.material.textfield.TextInputEditText
                            android:layout_width="match_parent"
                            android:fontFamily="@font/iransans"
                            android:gravity="right"
                            android:inputType="text"
                            android:id="@+id/edt_first_name"
                            android:layout_marginRight="16dp"
                            android:layout_marginLeft="16dp"
                            style="@style/H4Left"
                            android:backgroundTint="@android:color/transparent"
                            android:layout_height="match_parent"/>
                    </com.google.android.material.card.MaterialCardView>

                </androidx.constraintlayout.widget.ConstraintLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/layout_last_name"
                    android:layout_width="0dp"
                    android:layout_height="75dp"
                    android:layout_marginStart="@dimen/margin_normal"
                    android:layout_marginTop="21dp"
                    android:layout_marginEnd="@dimen/margin_normal"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/layout_first_name" >

                    <TextView
                        style="@style/H7Right"
                        android:id="@+id/tv_last_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/new_passenger_last_name"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <com.google.android.material.card.MaterialCardView
                        android:layout_marginTop="5dp"
                        app:strokeColor="#dfdfdf"
                        app:strokeWidth="1dp"
                        app:cardCornerRadius="10dp"
                        app:cardBackgroundColor="#e6e5ee"
                        android:id="@+id/card_last_name"
                        android:layout_width="0dp"
                        android:layout_height="48dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/tv_last_name" >
                        <com.google.android.material.textfield.TextInputEditText
                            android:layout_width="match_parent"
                            android:id="@+id/edt_last_name"
                            android:gravity="right"
                            android:inputType="text"
                            android:fontFamily="@font/iransans"
                            android:layout_marginRight="16dp"
                            android:layout_marginLeft="16dp"
                            style="@style/H4Left"
                            android:backgroundTint="@android:color/transparent"
                            android:layout_height="wrap_content"/>
                    </com.google.android.material.card.MaterialCardView>
                </androidx.constraintlayout.widget.ConstraintLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/layout_english_first_name"
                    android:layout_width="0dp"
                    android:layout_height="75dp"
                    android:layout_marginStart="@dimen/margin_normal"
                    android:layout_marginTop="21dp"
                    android:layout_marginEnd="@dimen/margin_normal"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/layout_last_name" >

                    <TextView
                        style="@style/H7Right"
                        android:id="@+id/tv_english_first_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/new_passenger_english_first_name"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <com.google.android.material.card.MaterialCardView
                        android:layout_marginTop="5dp"
                        app:strokeColor="#dfdfdf"
                        app:strokeWidth="1dp"
                        app:cardCornerRadius="10dp"
                        app:cardBackgroundColor="#e6e5ee"
                        android:id="@+id/card_english_first_name"
                        android:layout_width="0dp"
                        android:layout_height="48dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/tv_english_first_name" >
                        <com.google.android.material.textfield.TextInputEditText
                            android:layout_width="match_parent"
                            android:id="@+id/edt_english_first_name"
                            android:gravity="right"
                            android:inputType="text"
                            android:fontFamily="@font/iransans"
                            android:layout_marginRight="16dp"
                            android:layout_marginLeft="16dp"
                            style="@style/H4Left"
                            android:backgroundTint="@android:color/transparent"
                            android:layout_height="wrap_content"/>
                    </com.google.android.material.card.MaterialCardView>
                </androidx.constraintlayout.widget.ConstraintLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/layout_english_last_name"
                    android:layout_width="0dp"
                    android:layout_height="75dp"
                    android:layout_marginStart="@dimen/margin_normal"
                    android:layout_marginTop="21dp"
                    android:layout_marginEnd="@dimen/margin_normal"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/layout_english_first_name" >

                    <TextView
                        style="@style/H7Right"
                        android:id="@+id/tv_english_last_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/new_passenger_english_last_name"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <com.google.android.material.card.MaterialCardView
                        android:layout_marginTop="5dp"
                        app:strokeColor="#dfdfdf"
                        app:strokeWidth="1dp"
                        app:cardCornerRadius="10dp"
                        app:cardBackgroundColor="#e6e5ee"
                        android:id="@+id/card_english_last_name"
                        android:layout_width="0dp"
                        android:layout_height="48dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/tv_english_last_name" >
                        <com.google.android.material.textfield.TextInputEditText
                            android:layout_width="match_parent"
                            android:gravity="right"
                            android:inputType="text"
                           android:id="@+id/edt_english_last_name"
                            android:fontFamily="@font/iransans"
                            android:layout_marginRight="16dp"
                            android:layout_marginLeft="16dp"
                            style="@style/H4Left"
                            android:backgroundTint="@android:color/transparent"
                            android:layout_height="wrap_content"/>
                    </com.google.android.material.card.MaterialCardView>
                </androidx.constraintlayout.widget.ConstraintLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/layout_national_code"
                    android:layout_width="0dp"
                    android:layout_height="75dp"
                    android:layout_marginStart="@dimen/margin_normal"
                    android:layout_marginTop="21dp"
                    android:layout_marginEnd="@dimen/margin_normal"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/layout_english_last_name" >

                    <TextView
                        style="@style/H7Right"
                        android:id="@+id/tv_national_code"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/new_passenger_national_code"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <com.google.android.material.card.MaterialCardView
                        android:layout_marginTop="5dp"
                        app:strokeColor="#dfdfdf"
                        app:strokeWidth="1dp"
                        app:cardCornerRadius="10dp"
                        app:cardBackgroundColor="#e6e5ee"
                        android:id="@+id/card_national_code"
                        android:layout_width="0dp"
                        android:layout_height="48dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/tv_national_code" >
                        <com.google.android.material.textfield.TextInputEditText
                            android:maxLength="10"
                            android:layout_width="match_parent"
                            android:gravity="right"
                            android:id="@+id/edt_national_code"
                            android:inputType="number"
                            android:fontFamily="@font/iransans"
                            android:layout_marginRight="16dp"
                            android:layout_marginLeft="16dp"
                            style="@style/H4Left"
                            android:backgroundTint="@android:color/transparent"
                            android:layout_height="wrap_content"/>
                    </com.google.android.material.card.MaterialCardView>
                </androidx.constraintlayout.widget.ConstraintLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:visibility="gone"
                    android:id="@+id/layout_passport_number"
                    android:layout_width="0dp"
                    android:layout_height="75dp"
                    android:layout_marginStart="@dimen/margin_normal"
                    android:layout_marginTop="21dp"
                    android:layout_marginEnd="@dimen/margin_normal"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/layout_national_code" >

                    <TextView
                        style="@style/H7Right"
                        android:id="@+id/tv_passport_number"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/new_passenger_passport_number"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <com.google.android.material.card.MaterialCardView
                        android:layout_marginTop="5dp"
                        app:strokeColor="#dfdfdf"
                        app:strokeWidth="1dp"
                        app:cardCornerRadius="10dp"
                        app:cardBackgroundColor="#e6e5ee"
                        android:id="@+id/card_passport_number"
                        android:layout_width="0dp"
                        android:layout_height="48dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/tv_passport_number" >
                        <com.google.android.material.textfield.TextInputEditText
                            android:layout_width="match_parent"
                            android:gravity="right"
                            android:inputType="number"
                            android:id="@+id/edt_passport_number"
                            android:fontFamily="@font/iransans"
                            android:layout_marginRight="16dp"
                            android:layout_marginLeft="16dp"
                            style="@style/H4Left"
                            android:backgroundTint="@android:color/transparent"
                            android:layout_height="wrap_content"/>
                    </com.google.android.material.card.MaterialCardView>
                </androidx.constraintlayout.widget.ConstraintLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/layout_brithday"
                    android:layout_width="0dp"
                    android:layout_height="75dp"
                    android:layout_marginStart="@dimen/margin_normal"
                    android:layout_marginTop="21dp"
                    android:layout_marginEnd="@dimen/margin_normal"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/layout_passport_number" >

                    <TextView
                        style="@style/H7Right"
                        android:id="@+id/tv_brithday"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/new_passenger_date_of_birth"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <com.google.android.material.card.MaterialCardView
                        android:layout_marginTop="5dp"

                        app:strokeColor="#dfdfdf"
                        app:strokeWidth="1dp"
                        app:cardCornerRadius="10dp"
                        app:cardBackgroundColor="#e6e5ee"
                        android:id="@+id/card_birthday"
                        android:layout_width="0dp"
                        android:layout_height="48dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/tv_brithday" >
                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/calendarCard"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <ImageView
                                android:clickable="true"
                                android:background="?attr/selectableItemBackgroundBorderless"
                                android:focusable="true"
                                android:id="@+id/iv_calendar"
                                android:layout_width="24dp"
                                android:layout_height="24dp"
                                android:layout_marginStart="12dp"
                                android:src="@drawable/ic_calendar_fill"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent" />

                            <TextView
                                style="@style/H4Left"
                                android:id="@+id/tv_calendar"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="16dp"

                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintTop_toTopOf="parent" />
                        </androidx.constraintlayout.widget.ConstraintLayout>
                    </com.google.android.material.card.MaterialCardView>
                </androidx.constraintlayout.widget.ConstraintLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/layout_gender"
                    android:layout_width="0dp"
                    android:layout_height="38dp"
                    android:layout_marginStart="@dimen/margin_normal"
                    android:layout_marginTop="24dp"
                    android:layout_marginEnd="@dimen/margin_normal"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/layout_brithday" >

                    <LinearLayout
                        android:orientation="horizontal"
                        android:background="@drawable/item_passenger_radus"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="24dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toStartOf="@+id/tv_gender"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" >
                        <FrameLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/back_tabs">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="horizontal">

                                <TextView

                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:text=""
                                    />

                                <TextView
                                    android:id="@+id/select1"
                                    android:background="@drawable/back_select"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:text="" />

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="38dp"
                                android:orientation="horizontal">
                                <TextView
                                    android:textColor="@color/invalid_name_color"
                                    android:lineSpacingExtra="9sp"
                                    android:textSize="13sp"
                                    android:id="@+id/tv_man"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:text="@string/man"

                                    android:gravity="center"/>

                                <TextView
                                    android:lineSpacingExtra="9sp"
                                    android:textSize="13sp"
                                    android:textColor="@color/topaz"
                                    android:id="@+id/tv_woman"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:text="@string/woman"
                                    android:gravity="center"/>

                            </LinearLayout>
                        </FrameLayout>


                    </LinearLayout>

                    <TextView
                        style="@style/H5Right"
                        android:id="@+id/tv_gender"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/new_passenger_gender"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />



                </androidx.constraintlayout.widget.ConstraintLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/layout_mobile"
                    android:layout_width="0dp"
                    android:layout_height="75dp"
                    android:layout_marginStart="@dimen/margin_normal"
                    android:layout_marginTop="27dp"
                    android:layout_marginEnd="@dimen/margin_normal"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/layout_gender" >

                    <TextView
                        style="@style/H7Right"
                        android:id="@+id/tv_mobile"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/new_passenger_mobile_number"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <com.google.android.material.card.MaterialCardView
                        android:layout_marginTop="5dp"
                        app:strokeColor="#dfdfdf"
                        app:strokeWidth="1dp"
                        app:cardCornerRadius="10dp"
                        app:cardBackgroundColor="#e6e5ee"
                        android:id="@+id/card_mobile"
                        android:layout_width="0dp"
                        android:layout_height="48dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/tv_mobile" >
                        <com.google.android.material.textfield.TextInputEditText
                           android:maxLength="11"
                            android:layout_width="match_parent"
                            android:gravity="right"
                            android:id="@+id/edt_mobile"
                            android:inputType="number"
                            android:fontFamily="@font/iransans"
                            android:layout_marginRight="16dp"
                            android:layout_marginLeft="16dp"
                            style="@style/H4Left"
                            android:backgroundTint="@android:color/transparent"
                            android:layout_height="wrap_content"/>
                    </com.google.android.material.card.MaterialCardView>
                </androidx.constraintlayout.widget.ConstraintLayout>
                <com.google.android.material.button.MaterialButton
                    android:textColor="@color/white"
                    android:layout_marginBottom="30dp"
                    android:fontFamily="@font/iransans"
                    android:text="@string/register"
                    android:textStyle="bold"
                    android:lineSpacingExtra="10sp"
                    android:textSize="14sp"
                    android:textAlignment="center"
                    app:backgroundTint="@color/topaz"
                    app:cornerRadius="10dp"
                    android:id="@+id/btn_register"
                    android:layout_width="0dp"
                    android:layout_height="50dp"
                    android:layout_marginStart="@dimen/margin_normal"
                    android:layout_marginTop="30dp"
                    android:layout_marginEnd="@dimen/margin_normal"

                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/layout_mobile" >


                </com.google.android.material.button.MaterialButton>




            </androidx.constraintlayout.widget.ConstraintLayout>

        </ScrollView>




    </RelativeLayout>
</layout>